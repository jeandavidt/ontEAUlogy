@prefix : <http://example.org/ontocape-test/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Use normalized HTTP URIs
@prefix ontocape: <http://www.theworldavatar.com/ontology/ontocape/OntoCAPE/> .
@prefix system: <http://www.theworldavatar.com/ontology/ontocape/OntoCAPE/upper_level/system.owl#> .
@prefix net: <http://www.theworldavatar.com/ontology/ontocape/OntoCAPE/upper_level/network_system.owl#> .
@prefix phase: <http://www.theworldavatar.com/ontology/ontocape/OntoCAPE/material/phase_system/phase_system.owl#> .
@prefix substance: <http://www.theworldavatar.com/ontology/ontocape/OntoCAPE/material/substance/substance.owl#> .
@prefix model: <http://www.theworldavatar.com/ontology/ontocape/OntoCAPE/model/mathematical_model.owl#> .

# ========== TEST INSTANCES ==========

# Network structure
:WastewaterNetwork a net:NetworkSystem ;
    rdfs:label "Wastewater Treatment Network" .

:SourceNode a net:Node ;
    rdfs:label "Wastewater Source" ;
    net:partOfNetwork :WastewaterNetwork .

:TreatmentNode a net:Node ;
    rdfs:label "Treatment Plant" ;
    net:partOfNetwork :WastewaterNetwork .

:ReuseNode a net:Node ;
    rdfs:label "Agricultural Reuse" ;
    net:partOfNetwork :WastewaterNetwork .

:Flow1 a net:DirectedConnection ;
    rdfs:label "Raw wastewater flow" ;
    net:connectsFrom :SourceNode ;
    net:connectsTo :TreatmentNode .

:Flow2 a net:DirectedConnection ;
    rdfs:label "Treated water flow" ;
    net:connectsFrom :TreatmentNode ;
    net:connectsTo :ReuseNode .

# Material streams
:InflowStream a phase:LiquidMaterialStream ;
    rdfs:label "Wastewater inflow" ;
    phase:containsPhase :AqueousPhase .

:AqueousPhase a phase:LiquidPhase ;
    rdfs:label "Aqueous phase" ;
    phase:containsComponent :WaterComponent ;
    phase:containsComponent :CODComponent .

:WaterComponent a substance:ChemicalSpecies ;
    rdfs:label "Water" .

:CODComponent a substance:ChemicalSpecies ;
    rdfs:label "Chemical Oxygen Demand" .

# Quantities
:FlowRate_5000 a system:ScalarQuantity ;
    rdfs:label "Flow rate: 5000 mÂ³/day" ;
    system:numericalValue "5000.0"^^xsd:double .

# Mathematical model
:ASM1 a model:MathematicalModel ;
    rdfs:label "Activated Sludge Model No. 1" ;
    model:models :TreatmentNode ;
    model:hasModelVariable :SubstrateVar ;
    model:hasParameter :HRT_Param ;
    model:hasParameter :MuMax_Param .

:SubstrateVar a model:StateVariable ;
    rdfs:label "Substrate concentration" .

:HRT_Param a model:Parameter ;
    rdfs:label "Hydraulic retention time" ;
    model:isManipulable "true"^^xsd:boolean .

:MuMax_Param a model:Parameter ;
    rdfs:label "Maximum growth rate" ;
    model:isManipulable "false"^^xsd:boolean .
