@prefix : <http://test.example.org/onto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Ontology metadata
<http://test.example.org/onto> rdf:type owl:Ontology ;
    rdfs:label "Test Ontology"@en ;
    rdfs:comment "A minimal ontology for testing"@en ;
    owl:versionInfo "1.0.0" ;
    skos:definition "An ontology for testing the documentation generator."@en .

# Test Class
:TestClass rdf:type owl:Class ;
    rdfs:label "Test Class"@en ;
    rdfs:comment "A test class"@en .

:SubClass rdf:type owl:Class ;
    rdfs:label "Sub Class"@en ;
    rdfs:subClassOf :TestClass .

# Test Property
:testProperty rdf:type owl:ObjectProperty ;
    rdfs:label "test property"@en ;
    rdfs:domain :TestClass ;
    rdfs:range :SubClass .

# Test Individual
:testIndividual rdf:type owl:NamedIndividual , :TestClass ;
    rdfs:label "Test Individual"@en ;
    :testProperty :anotherIndividual ;
    rdfs:comment "An instance for testing"@en .

:anotherIndividual rdf:type owl:NamedIndividual , :SubClass ;
    rdfs:label "Another Individual"@en .
